{
  "distributed-cache": {
    "mode": "SYNC",
    "owners": 1,
    "statistics": true,
    "encoding": {
      "media-type": "application/x-protostream"
    },
    "locking": {
      "isolation": "REPEATABLE_READ"
    },
    "indexing": {
      "enabled": true,
      "storage": "filesystem",
      "startup-mode": "Purge",
      "indexed-entities": [
        "insights.book"
      ]
    }
  }
}